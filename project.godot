; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Line2D",
"class": "Edge",
"language": "GDScript",
"path": "res://Scenes/Scripts/Edge.gd"
}, {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
}, {
"base": "Node2D",
"class": "Level",
"language": "GDScript",
"path": "res://Scenes/Scripts/Level.gd"
}, {
"base": "Polygon2D",
"class": "Level_Node",
"language": "GDScript",
"path": "res://Scenes/Scripts/Node.gd"
} ]
_global_script_class_icons={
"Edge": "",
"GitAPI": "",
"Level": "",
"Level_Node": ""
}

[application]

config/name="Escape The Graph"
run/main_scene="res://Scenes/Game.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout="res://Assets/Sound/Audio Busses/default_bus_layout.tres"

[autoload]

Signals="*res://Scenes/Scripts/Signals.gd"
AudioHandler="*res://Scenes/Scripts/AudioHandler.gd"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
